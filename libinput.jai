    LIBINPUT_CONFIG_SCROLL_2FG            :: _2FG;
    LIBINPUT_CONFIG_SCROLL_EDGE           :: EDGE;
    LIBINPUT_CONFIG_SCROLL_ON_BUTTON_DOWN :: ON_BUTTON_DOWN;
}

